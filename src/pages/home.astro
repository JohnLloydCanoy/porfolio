---
import Tagline from "../components/tagline.astro";
import homeAsset from "../assets/Home-asset.png";
---

<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    </head>
    <body>
        <div class="flex items-center gap-6 flex-col md:flex-row">
            <Tagline />
        <img
        src={homeAsset.src}
        alt="Home Image"
        class="w-170 h-auto mt-6 rounded-lg "
        />
        </div>
        
    </body>
</html>
